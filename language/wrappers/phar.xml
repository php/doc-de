<refentry xml:id="wrappers.phar" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" annotations="verify_info:false" role="stream_wrapper">
 <refnamediv>
  <refname>phar://</refname>
  <refpurpose>PHP-Archiv</refpurpose>
 </refnamediv>

 <refsect1 role="description"><!-- {{{ -->
  &reftitle.description;
  <!-- TODO Import description? -->
  <simpara>
   Der Stream-Wrapper <filename>phar://</filename>.
   Eine ausführliche Beschreibung ist unter
   <link linkend="phar.using.stream">Phar-Stream-Wrapper</link> zu finden.
  </simpara>
 </refsect1><!-- }}} -->

 <refsect1 role="usage"> <!-- {{{ -->
  &reftitle.usage;
  <itemizedlist>
   <listitem><simpara><filename>phar://</filename></simpara></listitem>
  </itemizedlist>
 </refsect1> <!-- }}} -->

