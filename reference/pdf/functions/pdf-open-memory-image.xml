<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.pdf-open-memory-image">
 <refnamediv>
  <refname>PDF_open_memory_image</refname>
  <refpurpose>Öffnet mit PHP-Bildfunktionen erzeugtes Bild [nicht unterstützt]</refpurpose>
 </refnamediv>
 <refsect1 role="description">
   &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>PDF_open_memory_image</methodname>
   <methodparam><type>resource</type><parameter>p</parameter></methodparam>
   <methodparam><type>resource</type><parameter>image</parameter></methodparam>
  </methodsynopsis>
