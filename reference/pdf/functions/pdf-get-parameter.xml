<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.pdf-get-parameter">
 <refnamediv>
  <refname>PDF_get_parameter</refname>
  <refpurpose>Ermittelt String-Parameter</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>PDF_get_parameter</methodname>
   <methodparam><type>resource</type><parameter>p</parameter></methodparam>
   <methodparam><type>string</type><parameter>key</parameter></methodparam>
   <methodparam><type>float</type><parameter>modifier</parameter></methodparam>
  </methodsynopsis>
  <para>
   Ermittelt den Inhalt eines PDFlib-Parameters vom Typ String.
  </para>
 </refsect1>
</refentry>

