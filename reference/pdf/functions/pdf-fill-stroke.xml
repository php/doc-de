  <refentry id="function.pdf-fill-stroke">
   <refnamediv>
    <refname>PDF_fill_stroke</refname>
    <refpurpose>Zeichnet und füllt Pfad</refpurpose>
   </refnamediv>
   <refsect1 role="description">
    &reftitle.description;
     <methodsynopsis>
      <type>bool</type><methodname>PDF_fill_stroke</methodname>
      <methodparam><type>resource</type><parameter>p</parameter></methodparam>
     </methodsynopsis>
    <para>
     Zeichnet und füllt den Pfad mit der aktuellen Zeichen- und der aktuellen
     Füllfarbe. &return.success;
    </para>
   </refsect1>
  </refentry>

