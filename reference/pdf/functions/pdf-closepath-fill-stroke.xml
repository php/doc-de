<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.pdf-closepath-fill-stroke">
 <refnamediv>
  <refname>PDF_closepath_fill_stroke</refname>
  <refpurpose>Schließt, füllt und umrandet den aktuellen Pfad</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>PDF_closepath_fill_stroke</methodname>
   <methodparam><type>resource</type><parameter>p</parameter></methodparam>
  </methodsynopsis>
  <para>
   Schließt den Pfad, füllt und umrandet ihn.
   &return.success;
  </para>
 </refsect1>
</refentry>

