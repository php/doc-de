<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.pdf-utf16-to-utf8">
 <refnamediv>
  <refname>PDF_utf16_to_utf8</refname>
  <refpurpose>Konvertiert String von UTF-16 nach UTF-8</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>PDF_utf16_to_utf8</methodname>
   <methodparam><type>resource</type><parameter>pdfdoc</parameter></methodparam>
   <methodparam><type>string</type><parameter>utf16string</parameter></methodparam>
  </methodsynopsis>
  <para>
   Konvertiert einen String vom Format UTF-16 nach UTF-8.
  </para>
 </refsect1>
</refentry>


