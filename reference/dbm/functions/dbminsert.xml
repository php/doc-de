<!-- last change to 'dbminsert' in en/ tree in rev 1.2 -->
  <refentry id="function.dbminsert">
   <refnamediv>
    <refname>dbminsert</refname>
    <refpurpose>
     Schreibt einen Datensatz mit dem angegebenen Schlüssel
     in die DBM Datenbank.
    </refpurpose>
   </refnamediv>
   <refsect1>
    <title>Beschreibung</title>
     <methodsynopsis>
      <type>int</type><methodname>dbminsert</methodname>
      <methodparam><type>int</type><parameter>dbm_identifier</parameter></methodparam>
      <methodparam><type>string</type><parameter>key</parameter></methodparam>
      <methodparam><type>string</type><parameter>value</parameter></methodparam>
     </methodsynopsis>
    <para>
     Schreibt den Datensatz mit angegebenen Schlüssel in die
     Datenbank.
    </para>
    <para>
     Ergebniswerte:
     <variablelist>
      <varlistentry>
       <term>-1</term>
       <listitem><simpara>nur Leseberechtigung.</simpara>
       </listitem>
      </varlistentry>
      <varlistentry>
       <term>0</term>
       <listitem><simpara>Aktion erfolgreich beendet.</simpara>
       </listitem>
      </varlistentry>
      <varlistentry>
       <term>1</term>
       <listitem><simpara>Ein Eintrag mit dem angegebenen Schlüssel
        existiert bereits in der Datenbank.</simpara>
       </listitem>
      </varlistentry>
     </variablelist>
    </para>
   </refsect1>
  </refentry>

