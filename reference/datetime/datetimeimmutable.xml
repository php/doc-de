    </fieldsynopsis>
    <fieldsynopsis>
     <modifier>const</modifier>
     <type>string</type>
     <varname linkend="datetimeimmutable.constants.rfc2822">DateTimeImmutable::RFC2822</varname>
     <initializer>"D, d M Y H:i:s O"</initializer>
    </fieldsynopsis>
    <fieldsynopsis>
     <modifier>const</modifier>
     <type>string</type>
