  <refentry xml:id="function.dotnet-load" xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>dotnet_load</refname>
    <refpurpose>Lädt ein DOTNET-Modul</refpurpose>
   </refnamediv>
   <refsect1 role="description">
    &reftitle.description;
     <methodsynopsis>
      <type>int</type><methodname>dotnet_load</methodname>
      <methodparam><type>string</type><parameter>assembly_name</parameter></methodparam>
      <methodparam choice="opt"><type>string</type><parameter>datatype_name</parameter></methodparam>
      <methodparam choice="opt"><type>int</type><parameter>codepage</parameter></methodparam>
     </methodsynopsis>
     &warn.experimental.func;
     &warn.undocumented.func;
   </refsect1>
