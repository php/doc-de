  <refentry xml:id='function.domattribute-name' xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>DomAttribute->name</refname>
    <refpurpose>
     Gibt den Namen eines Attributes zurück.
    </refpurpose>
   </refnamediv>
   <refsect1 role="description">
    &reftitle.description;
    <classsynopsis>
     <ooclass><classname>DomAttribute</classname></ooclass>
      <methodsynopsis>
      <type>string</type><methodname>name</methodname>
       <void/>
      </methodsynopsis>
    </classsynopsis>
    <para>
     Diese Funktion gibt den Namen eines Attributes zurück.
    </para>
   </refsect1>
   <refsect1 role="returnvalues">
    &reftitle.returnvalues;
    <para>
     Gibt den Namen des Attributes zurück.
    </para>
   </refsect1>
   <refsect1 role="migration">
    <title>Migration zu PHP 5</title>
    <para>
     Nutzung der <property>name</property>-Eigenschaft von
     <classname>DOMAttr</classname>.
    </para>
   </refsect1>
   <refsect1 role="seealso">
    &reftitle.seealso;
    <para>
     <simplelist>
      <member><xref linkend="function.domattribute-value" /> für ein Beispiel</member>
     </simplelist>
    </para>
   </refsect1>
  </refentry>

