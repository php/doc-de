<!-- splitted from ./de/functions/uodbc.xml, last change in rev 1.2 -->
<!-- last change to 'odbc-cursor' in en/ tree in rev 1.2 -->
  <refentry id="function.odbc-cursor">
   <refnamediv>
    <refname>odbc_cursor</refname>
    <refpurpose>Findet den Cursornamen heraus</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Beschreibung</title>
     <methodsynopsis>
      <type>string</type><methodname>odbc_cursor</methodname>
      <methodparam><type>int</type><parameter>result_id</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>odbc_cursor</function> liefert einen Cursornamen für
     die gegebene Ergebniskennung <parameter>result_id</parameter>
     zurück.
    </para>
   </refsect1>
  </refentry>

