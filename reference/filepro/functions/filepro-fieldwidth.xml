<!-- last change to 'filepro-fieldwidth' in en/ tree in rev 1.2 -->
  <refentry id="function.filepro-fieldwidth">
   <refnamediv>
    <refname>filepro_fieldwidth</refname>
    <refpurpose>ermittelt die Breite eines Feldes</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Beschreibung</title>
     <methodsynopsis>
      <type>int</type><methodname>filepro_fieldwidth</methodname>
      <methodparam><type>int</type><parameter>field_number</parameter></methodparam>
     </methodsynopsis>
    <para>
     Gibt die Breite des Feldes, das <parameter>field_number</parameter>
     entspricht, zurück.
    </para>
   </refsect1>
  </refentry>

