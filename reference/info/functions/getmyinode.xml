<refentry xml:id="function.getmyinode" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>getmyinode</refname>
  <refpurpose>Ermittelt den Inode eines Skripts</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>getmyinode</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Ermittelt den Inode der aktuell ausgeführten Skriptdatei.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Gibt die Inode-Nummer der aktuellen Skriptdatei als Integer
   zurück oder &false; bei fehlern.
  </para>
 </refsect1>

 <refsect1 role="notes">
  &reftitle.notes;
  &note.no-windows;
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>getmygid</function></member>
    <member><function>getmyuid</function></member>
    <member><function>getmypid</function></member>
    <member><function>get_current_user</function></member>
    <member><function>getlastmod</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

