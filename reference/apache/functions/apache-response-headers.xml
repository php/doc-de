<refentry xml:id="function.apache-response-headers" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>apache_response_headers</refname>
  <refpurpose>Liefert alle HTTP-Responseheader</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type class="union"><type>array</type><type>false</type></type><methodname>apache_response_headers</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Liefert alle HTTP-Responseheader.
   Verf√ºgbar unter Apache-, FastCGI-, CLI- und FPM-Webservern.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Ein Array aller Apache-Responseheader im Erfolgsfall. &return.falseforfailure;
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><function>apache_response_headers</function>-Beispiel</title>
    <programlisting role="php">
<![CDATA[
<?php
print_r(apache_response_headers());
?>
]]>
    </programlisting>
    &example.outputs.similar;
    <screen>
<![CDATA[
Array
(
    [Accept-Ranges] => bytes
    [X-Powered-By] => PHP/4.3.8
)
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>apache_request_headers</function></member>
    <member><function>headers_sent</function></member>
    <member><function>headers_list</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>


