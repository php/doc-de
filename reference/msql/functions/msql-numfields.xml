  <refentry id='function.msql-numfields'>
   <refnamediv>
    <refname>msql_numfields</refname>
    <refpurpose>Gibt die Anzahl der Felder eines Abfrageergebnisses zurück</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Beschreibung</title>
     <methodsynopsis>
      <type>int</type><methodname>msql_numfields</methodname>
      <methodparam><type>int</type><parameter>query_identifier</parameter></methodparam>
     </methodsynopsis>
    <para>
     Identisch zu <function>msql_num_fields</function>.
    </para>
   </refsect1>
  </refentry>

