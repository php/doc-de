  <refentry id="function.rename">
   <refnamediv>
    <refname>rename</refname>
    <refpurpose>Benennt eine Datei um</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Beschreibung</title>
     <methodsynopsis>
      <type>bool</type><methodname>rename</methodname>
      <methodparam><type>string</type><parameter>oldname</parameter></methodparam>
      <methodparam><type>string</type><parameter>newname</parameter></methodparam>
     </methodsynopsis>
    <para>
     Versucht eine Datei <parameter>oldname</parameter> in
     <parameter>newname</parameter> umzubenennen.
    </para>
    <para>
     &return.success;
    <example>
     <title>Beispiel mit <function>rename</function></title>
     <programlisting role="php">
<![CDATA[
<?php
  rename("/tmp/tmp_file.txt", "/home/user/login/docs/my_file.txt");
?>
]]>
     </programlisting>
    </example>
    </para>
   </refsect1>
  </refentry>

