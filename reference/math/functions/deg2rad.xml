<?xml version="1.0" encoding="iso-8859-1"?>
<!-- splitted from ./de/functions/math.xml, last change in rev 1.8 -->
<!-- last change to 'deg2rad' in en/ tree in rev 1.2 -->
  <refentry id="function.deg2rad">
   <refnamediv>
    <refname>deg2rad</refname>
    <refpurpose>Grad in Bogenmaß wandeln</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Beschreibung</title>
     <methodsynopsis>
      <type>double</type><methodname>deg2rad</methodname>
      <methodparam><type>double</type><parameter>number</parameter></methodparam>
     </methodsynopsis>
    <para>
     Diese Funktion wandelt den übergebenen Winkel von Grad in
     Bogenmaß.
    </para>
    <para>
     Siehe auch <function>rad2deg</function>.
    </para>
   </refsect1>
  </refentry>

