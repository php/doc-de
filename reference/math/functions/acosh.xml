<refentry xml:id="function.acosh" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>acosh</refname>
  <refpurpose>Areakosinus Hyperbolikus</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
   <methodsynopsis>
    <type>float</type><methodname>acosh</methodname>
    <methodparam><type>float</type><parameter>arg</parameter></methodparam>
   </methodsynopsis>
  <para>
   Gibt den inversen Kosinus Hyperbolikus von <parameter>arg</parameter>
   zurück, d.h. den Wert dessen Kosinus Hyperbolikus <parameter>arg</parameter>
   ergibt.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>arg</parameter></term>
     <listitem>
      <para>
       Der zu verarbeitende Wert
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Der Areakosinus Hyperbolikus von <parameter>arg</parameter>
  </para>
 </refsect1>

 <refsect1 role="changelog">
  &reftitle.changelog;
  <para>
   <informaltable>
    <tgroup cols="2">
     <thead>
      <row>
       <entry>&Version;</entry>
       <entry>&Description;</entry>
      </row>
     </thead>
     <tbody>
      <row>
       <entry>5.3.0</entry>
       <entry>
        Die Funktion ist nun auf allen Plattformen verfügbar.
       </entry>
      </row>
     </tbody>
    </tgroup>
   </informaltable>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>cosh</function></member>
    <member><function>acos</function></member>
    <member><function>asinh</function></member>
    <member><function>atanh</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>


