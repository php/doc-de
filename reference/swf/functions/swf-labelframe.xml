<!-- last change to 'swf-labelframe' in en/ tree in rev 1.8 -->
  <refentry id="function.swf-labelframe">
   <refnamediv>
    <refname>swf_labelframe</refname>
    <refpurpose>Beschriftet den aktuellen Frame</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Beschreibung</title>
     <methodsynopsis>
      <type>void</type><methodname>swf_labelframe</methodname>
      <methodparam><type>string</type><parameter>name</parameter></methodparam>
     </methodsynopsis>
    <para>
	 Beschriftet den aktuellen Frame mit der Bezeichnung,
	 die durch das Parameter <parameter>name</parameter>
	 angegeben wurde.
    </para>
   </refsect1>
  </refentry>

