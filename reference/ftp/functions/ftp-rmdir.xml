<!-- splitted from ./de/functions/ftp.xml, last change in rev 1.2 -->
<!-- last change to 'ftp-rmdir' in en/ tree in rev 1.2 -->
  <refentry id="function.ftp-rmdir">
   <refnamediv>
    <refname>ftp_rmdir</refname>
    <refpurpose>Löscht ein Verzeichnis</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Beschreibung:</title>
     <methodsynopsis>
      <type>int</type><methodname>ftp_rmdir</methodname>
      <methodparam><type>int</type><parameter>ftp_stream</parameter></methodparam>
      <methodparam><type>string</type><parameter>Verzeichnis</parameter></methodparam>
     </methodsynopsis>
    <para>
     Bei Erfolg wird &true;, ansonsten &false; zurück gegeben.
    </para>
    <para> 
     Löscht das angegebene <parameter>Verzeichnis</parameter>.
    </para>
   </refsect1>
  </refentry>

