<?xml version="1.0" encoding="iso-8859-1"?>
<!-- splitted from ./de/functions/ftp.xml, last change in rev 1.2 -->
<!-- last change to 'ftp-delete' in en/ tree in rev 1.2 -->
  <refentry id="function.ftp-delete">
   <refnamediv>
    <refname>ftp_delete</refname>
    <refpurpose>Löscht eine Datei auf dem FTP-Server</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Beschreibung:</title>
     <methodsynopsis>
      <type>int</type><methodname>ftp_delete</methodname>
      <methodparam><type>int</type><parameter>ftp_stream</parameter></methodparam>
      <methodparam><type>string</type><parameter>path</parameter></methodparam>
     </methodsynopsis>
    <para>
     Falls erfolgreich wird &true;, sonst &false; zurück gegeben.
    </para>
    <para>
     <function>ftp_delete</function> löscht die per
     <parameter>path</parameter> angegebene Datei auf dem FTP-Server.
    </para>
   </refsect1>
  </refentry>

