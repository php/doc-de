  <refentry id="function.ftp-rename">
   <refnamediv>
    <refname>ftp_rename</refname>
    <refpurpose>Benennt eine Datei auf dem FTP-Server um</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Beschreibung</title>
     <methodsynopsis>
      <type>int</type><methodname>ftp_rename</methodname>
      <methodparam><type>int</type><parameter>ftp_stream</parameter></methodparam>
      <methodparam><type>string</type><parameter>from</parameter></methodparam>
      <methodparam><type>string</type><parameter>to</parameter></methodparam>
     </methodsynopsis>
    <para>
     &return.success;
    </para>
    <para>
     <function>ftp_rename</function> benennt die mit
     <parameter>from</parameter> angegebene Datei in
     <parameter>to</parameter>um.
    </para>
   </refsect1>
  </refentry>

