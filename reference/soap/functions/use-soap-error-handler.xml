   <type>bool</type><methodname>use_soap_error_handler</methodname>
   <methodparam choice="opt"><type>bool</type><parameter>handler</parameter></methodparam>
  </methodsynopsis>
  &warn.undocumented.func;
 </refsect1>
</refentry>

