<refentry xml:id="function.xmlwriter-end-attribute" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>XMLWriter->endAttribute</refname>
  <refpurpose>Attribute beenden</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <para>Objektorientiert:</para>
  <classsynopsis>
   <ooclass><classname>XMLWriter</classname></ooclass>
   <methodsynopsis>
    <type>bool</type><methodname>endAttribute</methodname>
    <void/>
   </methodsynopsis>
  </classsynopsis>
  <para>Prozedural:</para>
  <methodsynopsis>
   <type>bool</type><methodname>xmlwriter_end_attribute</methodname>
   <methodparam><type>resource</type><parameter>xmlwriter</parameter></methodparam>
  </methodsynopsis>
  <para>
   Beendet das aktuelle Attribut.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
