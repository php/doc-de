<refentry xml:id="function.xmlwriter-text" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>XMLWriter->text</refname>
  <refpurpose>Text schreiben</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <para>Objektorientiert:</para>
  <classsynopsis>
   <ooclass><classname>XMLWriter</classname></ooclass>
   <methodsynopsis>
    <type>bool</type><methodname>text</methodname>
    <methodparam><type>string</type><parameter>content</parameter></methodparam>
   </methodsynopsis>
  </classsynopsis>
  <para>Prozedural:</para>
  <methodsynopsis>
   <type>bool</type><methodname>xmlwriter_text</methodname>
   <methodparam><type>resource</type><parameter>xmlwriter</parameter></methodparam>
   <methodparam><type>string</type><parameter>content</parameter></methodparam>
  </methodsynopsis>
  <para>
   Schreibt einen Text.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    &xmlwriter.xmlwriter.description;
    <varlistentry>
     <term><parameter>content</parameter></term>
     <listitem>
      <para>
       Textinhalt.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

</refentry>

