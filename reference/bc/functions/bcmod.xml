  <refentry id="function.bcmod">
   <refnamediv>
    <refname>bcmod</refname>
    <refpurpose>
     Modulo zweier Zahlen mit beliebiger Genauigkeit.
    </refpurpose>
   </refnamediv>
   <refsect1>
    <title>Beschreibung</title>
     <methodsynopsis>
      <type>string</type><methodname>bcmod</methodname>
      <methodparam><type>string</type><parameter>linker Operand</parameter></methodparam>
      <methodparam><type>string</type><parameter>Modulus</parameter></methodparam>
     </methodsynopsis>
    <para>
     Liefert den <parameter>linken Operanden</parameter> modulo
     <parameter>Modulus</parameter>.
    </para>
    <para>
     Siehe auch <function>bcdiv</function>.
    </para>
   </refsect1>
  </refentry>

