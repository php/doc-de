<refentry xml:id="function.session-status" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>session_status</refname>
  <refpurpose>Gibt den Status der aktuellen Sitzung zurück</refpurpose>
 </refnamediv>

 <refsect1 role="description"><!-- {{{ -->
  &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>session_status</methodname>
   <void/>
  </methodsynopsis>
  <para>
   <function>session_status</function> wird für die Rückgabe des Status der aktuellen Sitzung verwendet.
  </para>
 </refsect1><!-- }}} -->

 <refsect1 role="returnvalues"><!-- {{{ -->
  &reftitle.returnvalues;
  <para>
   <itemizedlist>
    <listitem>
     <simpara><constant>PHP_SESSION_DISABLED</constant> wenn die Sitzung deaktiviert ist.</simpara>
    </listitem>
    <listitem>
     <simpara><constant>PHP_SESSION_NONE</constant> wenn Sitzungen aktiviert sind, aber keine existiert.</simpara>
    </listitem>
    <listitem>
     <simpara><constant>PHP_SESSION_ACTIVE</constant> wenn Sitzungen aktiviert sind und eine existiert.</simpara>
    </listitem>
   </itemizedlist>
  </para>
 </refsect1><!-- }}} -->
 
 <refsect1 role="seealso"><!-- {{{ -->
  &reftitle.seealso;
  <simplelist>
   <member><function>session_start</function></member>
  </simplelist>
 </refsect1><!-- }}} -->

</refentry>

