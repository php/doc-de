  <refentry xml:id="function.fbsql-start-db" xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>fbsql_start_db</refname>
    <refpurpose>Startet eine Datenbank auf einem lokalen oder entfernten Server</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Beschreibung</title>
     <methodsynopsis>
      <type>bool</type><methodname>fbsql_start_db</methodname>
      <methodparam><type>string</type><parameter>Datenbankname</parameter></methodparam>
      <methodparam choice="opt"><type>resource</type><parameter>
        Verbindungskennung
       </parameter></methodparam>
     </methodsynopsis>
    <para> 
     &return.success;
    </para>
    <para> 
     <function>fbsql_start_db</function>
    </para>
   </refsect1>

   <refsect1 xml:id="function.fbsql-start-db.seealso">
    &reftitle.seealso;
    <para> 
     <function>fbsql_db_status</function> und
     <function>fbsql_stop_db</function>.
    </para>
   </refsect1>
  </refentry>

