<refentry xml:id="function.com-addref" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>com_addref</refname>
  <refpurpose>Erhöht den Referenzzähler der Komponente [veraltet, nicht mehr empfohlen]</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
    <type>void</type><methodname>com_addref</methodname>
    <void/>
  </methodsynopsis>
  <para>
   Erhöht den Referenzzähler der Komponente.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>

 <refsect1 role="notes">
  &reftitle.notes;
  <warning>
   <simpara>
    Sie sollten keine Verwendung für diese Funktion mehr haben.
   </simpara>
  </warning>
 </refsect1>
</refentry>


