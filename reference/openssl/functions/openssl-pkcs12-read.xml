<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.openssl-pkcs12-read">
 <refnamediv>
  <refname>openssl_pkcs12_read</refname>
  <refpurpose>Speichert ein PKCS#12 Zertifikat in einem Array</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>openssl_pkcs12_read</methodname>
   <methodparam><type>mixed</type><parameter>PKCS12</parameter></methodparam>
   <methodparam><type>array</type><parameter role="reference">certs</parameter></methodparam>
   <methodparam><type>string</type><parameter>pass</parameter></methodparam>
  </methodsynopsis>
  <para>
   <function>openssl_pkcs12_read</function> speichert das im Parameter <parameter>PKCS12</parameter>
   übergebene PKCS#12 Zertifikat als Array im Parameter <parameter>certs</parameter>.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>PKCS12</parameter></term>
     <listitem>
      <para>
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>certs</parameter></term>
     <listitem>
      <para>
       Enthält im Erfolgsfall die Zertifikatsdaten als Array.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>pass</parameter></term>
     <listitem>
      <para>
       Passwort zum Entschlüsseln der PKCS#12-Datei.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

</refentry>

