   </tgroup>
  </informaltable>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><function>end</function>-Beispiel</title>
    <programlisting role="php">
<![CDATA[
<?php

$fruits = array('apple', 'banana', 'cranberry');
echo end($fruits); // cranberry

?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>

