  <refentry xml:id="function.dns-check-record" xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>dns_check_record</refname>
    <refpurpose>Synonym für <function>checkdnsrr</function></refpurpose>
   </refnamediv>
   <refsect1 role="description">
    &reftitle.description;
     <methodsynopsis>
      <type>bool</type><methodname>dns_check_record</methodname>
      <methodparam><type>string</type><parameter>host</parameter></methodparam>
      <methodparam choice="opt"><type>string</type><parameter>type</parameter></methodparam>
     </methodsynopsis>
    <para>
      Überprüft die zum übergebenen Hostnamen oder zur übergebenen IP-Adresse
      passenden DNS-Records.
    </para>
   </refsect1>
  </refentry>

