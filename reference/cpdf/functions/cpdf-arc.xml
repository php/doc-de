<refentry xml:id="function.cpdf-arc" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>cpdf_arc</refname>
  <refpurpose>Zeichnet einen Kreisbogen</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>cpdf_arc</methodname>
   <methodparam><type>int</type><parameter>PDF-Dokument</parameter></methodparam>
   <methodparam><type>float</type><parameter>x_koor</parameter></methodparam>
   <methodparam><type>float</type><parameter>y_koor</parameter></methodparam>
   <methodparam><type>float</type><parameter>Radius</parameter></methodparam>
   <methodparam><type>float</type><parameter>Anfang</parameter></methodparam>
   <methodparam><type>float</type><parameter>Ende</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>Modus</parameter></methodparam>
  </methodsynopsis>
  <para>
   Zeichnet einen Kreisbogen.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    &cpdf.pdf-document.description;
    <varlistentry>
     <term><parameter>x_koor</parameter></term>
     <listitem>
      <para>
       Die x-Koordinate des Mittelpunktes
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>y_koor</parameter></term>
     <listitem>
      <para>
       Die y-Koordinate des Mittelpunktes
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>Radius</parameter></term>
     <listitem>
      <para>
       Der Radius des Kreisbogens
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>Anfang</parameter></term>
     <listitem>
      <para>
       Der Anfangswinkel in Grad
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>Ende</parameter></term>
     <listitem>
      <para>
       Der Endwinkel in Grad
      </para>
     </listitem>
    </varlistentry>
    &cpdf.mode.description;
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>cpdf_circle</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

