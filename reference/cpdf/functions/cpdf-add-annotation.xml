<refentry id="function.cpdf-add-annotation">
 <refnamediv>
  <refname>cpdf_add_annotation</refname>
  <refpurpose>Fügt eine Anmerkung ein</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>cpdf_add_annotation</methodname>
   <methodparam><type>int</type><parameter>PDF-Dokument</parameter></methodparam>
   <methodparam><type>float</type><parameter>ulx</parameter></methodparam>
   <methodparam><type>float</type><parameter>uly</parameter></methodparam>
   <methodparam><type>float</type><parameter>orx</parameter></methodparam>
   <methodparam><type>float</type><parameter>ory</parameter></methodparam>
   <methodparam><type>string</type><parameter>Titel</parameter></methodparam>
   <methodparam><type>string</type><parameter>Inhalt</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>Modus</parameter></methodparam>
  </methodsynopsis>
  <para>
   Fügt eine Anmerkung am angegebenen Punkt ein.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    &cpdf.pdf-document.description;
    <varlistentry>
     <term><parameter>ulx</parameter></term>
     <listitem>
      <para>
       Die x-Koordinate der Ecke unten links
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>uly</parameter></term>
     <listitem>
      <para>
       Die y-Koordinate der Ecke unten links
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>orx</parameter></term>
     <listitem>
      <para>
       Die x-Koordinate der Ecke oben rechts
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>ory</parameter></term>
     <listitem>
      <para>
       Die y-Koordinate der Ecke oben rechts
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>Titel</parameter></term>
     <listitem>
      <para>
       Der Titel der Anmerkung
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>Inhalt</parameter></term>
     <listitem>
      <para>
       Der Inhalt der Anmerkung
      </para>
     </listitem>
    </varlistentry>
    &cpdf.mode.description;
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>
</refentry>

