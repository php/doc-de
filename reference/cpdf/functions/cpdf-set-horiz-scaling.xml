<refentry id="function.cpdf-set-horiz-scaling">
 <refnamediv>
  <refname>cpdf_set_horiz_scaling</refname>
  <refpurpose>Setzt die horizontale Textskalierung</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>cpdf_set_horiz_scaling</methodname>
   <methodparam><type>int</type><parameter>PDF-Dokument</parameter></methodparam>
   <methodparam><type>float</type><parameter>Skalierung</parameter></methodparam>
  </methodsynopsis>
  <para>
   Setzt die horizontale Skalierung auf <parameter>Skalierung</parameter>
   Prozent.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    &cpdf.pdf-document.description;
    <varlistentry>
     <term><parameter>Skalierung</parameter></term>
     <listitem>
      <para>
       Die Skalierung in Prozent
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>
</refentry>

